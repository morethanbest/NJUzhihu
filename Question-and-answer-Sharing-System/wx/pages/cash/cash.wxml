    </view>
    <view class="ans-wrapper">
      <toast hidden="{{hiddenToast}}" duration="2000" bindchange="toastHidden">感谢您的理解！</toast>
    </view>
    <view class="quiz-wrapper">
      <form bindsubmit="bindSubmit">
        <button class="submit" form-type="submit">我已知晓</button>
      </form>
    </view>
  </view>
</view>